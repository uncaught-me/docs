{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "mint",
  "name": "Uncaught - документация",
  "colors": {
    "primary": "#52525b",
    "light": "#a8a29e",
    "dark": "#292524"
  },
  "favicon": "/favicon.svg",
  "navigation": {
    "languages": [
      {
        "language": "ru",
        "groups": [
          {
            "group": "Браузерное приложение",
            "pages": ["apps/browser"]
          },
          {
            "group": "Overlay",
            "pages": [
              "apps/overlay",
              "apps/overlay-security",
              "apps/overlay-hotkeys",
              "apps/overlay-troubleshooting"
            ]
          },
          {
            "group": "Промпты",
            "pages": ["prompts/start", "prompts/usage"]
          }
        ]
      }
    ],
    "global": {
      "anchors": [
        {
          "anchor": "Новости",
          "href": "https://t.me/uncaughtme",
          "icon": "newspaper"
        },
        {
          "anchor": "Поддержка",
          "href": "https://t.me/uncaughtsupport",
          "icon": "user"
        }
      ]
    }
  },
  "logo": {
    "light": "/logo/light.svg",
    "dark": "/logo/dark.svg"
  },
  "navbar": {
    "links": [
      {
        "label": "Обновления",
        "href": "https://t.me/uncaughtme"
      }
    ]
  },
  "footer": {
    "socials": {
      "telegram": "https://t.me/uncaughtme"
    }
  }
}
